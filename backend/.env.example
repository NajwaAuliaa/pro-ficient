# ===============================================Azure OpenAI===============================================
azure-openai-api-key=YOUR_AZURE_OPENAI_API_KEY
azure-openai-endpoint=https://YOUR_RESOURCE.openai.azure.com/
azure-openai-api-version=2024-12-01-preview
azure-openai-deployment=gpt-4.1
azure-openai-embed-deployment=text-embedding-3-large

# ===============================================Azure Blob Storage===============================================
azure-blob-connection-string=YOUR_AZURE_BLOB_CONNECTION_STRING
azure-blob-container=YOUR_CONTAINER_NAME

# ===============================================Azure AI Document Intelligence===============================================
azure-docint-endpoint=https://YOUR_RESOURCE.cognitiveservices.azure.com/
azure-docint-key=YOUR_DOCUMENT_INTELLIGENCE_KEY

# ===============================================Microsoft Graph API (Planner)===============================================
ms-client-id=YOUR_CLIENT_ID
ms-client-secret=YOUR_CLIENT_SECRET
ms-tenant-id=YOUR_TENANT_ID
ms-graph-scope=https://graph.microsoft.com/.default

# ===============================================Azure Function (Preprocess)===============================================
azure-function-preprocess-url=
azure-function-preprocess-key=

# ===============================================Azure SQL (optional)===============================================
azure-sql-server=tcp:YOUR_SQL_SERVER.database.windows.net,1433
azure-sql-database=YOUR_DB_NAME
azure-sql-username=YOUR_USERNAME
azure-sql-password=YOUR_PASSWORD

# ===============================================Notifications===============================================
notify-webhook-url=https://YOUR_WEBHOOK_URL

# ===============================================App===============================================
app-debug=false
azure-redirect-uri=YOUR_REDIRECT_URI

# ===============================================Qdrant Vector Search===============================================
qdrant-url=YOUR_QDRANT_URL
qdrant-api-key=YOUR_QDRANT_API_KEY
qdrant-collection=internal-docs-index

# ===============================================Azure Cache for Redis===============================================
redis-host=YOUR_REDIS_HOST.redis.cache.windows.net
redis-port=6380
redis-password=YOUR_REDIS_PASSWORD
redis-ssl=true

# ===============================================Azure Cosmos DB===============================================
cosmos-endpoint=https://YOUR_COSMOS_ACCOUNT.documents.azure.com:443/
cosmos-key=YOUR_COSMOS_KEY
cosmos-database=internal_assistant
cosmos-container=conversation_history
